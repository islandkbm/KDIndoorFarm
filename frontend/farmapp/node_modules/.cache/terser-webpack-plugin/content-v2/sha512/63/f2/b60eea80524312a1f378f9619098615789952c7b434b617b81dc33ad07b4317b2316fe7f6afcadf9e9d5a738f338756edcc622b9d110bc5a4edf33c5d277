ytimer();\r\n      } else if (this.mConfig.pwmcontrolenable === true) {\r\n        mstatus = this.controlbypwm();\r\n      } else {\r\n        mstatus = this.controlbysensor(msensors);\r\n      }\r\n    } else {\r\n      mstatus = false;\r\n    }\r\n    return this.mState.ischangestatecheck(mstatus);\r\n  }\r\n};\r\n","\r\n\r\n\r\n class responseMessage{\r\n    \r\n    constructor()\r\n    {\r\n        let today = new Date(); \r\n        this.datetime=today.toLocaleString();//응답된 날자+시간\r\n        this.Sensors=[];\r\n        this.Outputs=[];\r\n        this.AutoStatus=[];\r\n        this.AutoControls=[];\r\n        this.IsOK=false; ///요청을 정상적으로 처리했으면 true\r\n    }\r\n\r\n}\r\nmodule.exports = responseMessage;"]}}